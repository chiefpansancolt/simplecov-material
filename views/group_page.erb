<div class="tab-groups" name="<%= title_id %>" style="<%= hide_show(title_id) %>">
  <div class="mdc-layout-grid">
    <div class="mdc-layout-grid__inner">
      <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-4-tablet">
        <div class="mdc-card card-tile">
          <p class="card-title"># of Files</p>
          <h4 class="card-data mdc-typography--headline4">
            <%= format_number(files.length) %>
          </h4>
        </div>
      </div>
      <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-4-tablet">
        <div class="mdc-card card-tile">
          <p class="card-title">Relevent Lines</p>
          <h4 class="card-data mdc-typography--headline4">
            <%= format_number(files.lines_of_code) %>
          </h4>
        </div>
      </div>
      <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-4-tablet">
        <div class="mdc-card card-tile">
          <p class="card-title">Lines Covered</p>
          <h4 class="card-data mdc-typography--headline4 green">
            <%= format_number(files.covered_lines) %>
          </h4>
        </div>
      </div>
      <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-4-tablet">
        <div class="mdc-card card-tile">
          <p class="card-title">Lines Missed</p>
          <h4 class="card-data mdc-typography--headline4 red">
            <%= format_number(files.missed_lines) %>
          </h4>
        </div>
      </div>
      <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-4-tablet">
        <div class="mdc-card card-tile">
          <p class="card-title">Covered</p>
          <h4 class="card-data mdc-typography--headline4 <%= coverage_class(files.covered_percent) %>">
              <%= files.covered_percent.round(2) %>%
          </h4>
        </div>
      </div>
      <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-4-tablet">
        <div class="mdc-card card-tile">
          <p class="card-title">Hits/Line</p>
          <h4 class="card-data mdc-typography--headline4 <%= strength_class(files.covered_strength) %>">
              <%= files.covered_strength.round(2) %>
          </h4>
        </div>
      </div>
    </div>
    <% if branchable_result? %>
      <div class="mdc-layout-grid__inner" style="padding-top: 10px;">
        <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-4-tablet">
          <div class="mdc-card card-tile">
            <p class="card-title">Total Branches</p>
            <h4 class="card-data mdc-typography--headline4">
              <%= format_number(files.total_branches) %>
            </h4>
          </div>
        </div>
        <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-4-tablet">
          <div class="mdc-card card-tile">
            <p class="card-title">Branches Covered</p>
            <h4 class="card-data mdc-typography--headline4 green">
              <%= format_number(files.covered_branches) %>
            </h4>
          </div>
        </div>
        <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-4-tablet">
          <div class="mdc-card card-tile">
            <p class="card-title">Branches Missed</p>
            <h4 class="card-data mdc-typography--headline4 red">
              <%= format_number(files.missed_branches) %>
            </h4>
          </div>
        </div>
        <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-4-tablet">
          <div class="mdc-card card-tile">
            <p class="card-title">Branches</p>
            <h4 class="card-data mdc-typography--headline4 <%= coverage_class(files.covered_percent) %>">
              <%= files.branch_covered_percent.round(2) %>%
            </h4>
          </div>
        </div>
      </div>
    <% end %>
  </div>

  <div class="text-field-container search-box-outline">
    <div class="mdc-text-field text-field mdc-text-field-outlined-shaped mdc-text-field--outlined mdc-text-field--with-leading-icon">
      <i class="material-icons mdc-text-field__icon">search</i>
      <input type="search" class="mdc-text-field__input search-box" onkeyup="searchTable(this.id)" onsearch="searchTable(this.id)" id="<%= title_id %>">
      <div class="mdc-notched-outline">
        <div class="mdc-notched-outline__leading"></div>
        <div class="mdc-notched-outline__notch">
          <label for="tf-outlined" class="mdc-floating-label">Search</label>
        </div>
        <div class="mdc-notched-outline__trailing"></div>
      </div>
    </div>
    <div class="mdc-text-field-helper-line">
      <div class="mdc-text-field-helper-text mdc-text-field-helper-text--persistent mdc-text-field-helper-text--validation-msg">You can search based on File column.</div>
    </div>
  </div>

  <br/>

  <div class="mdc-data-table">
    <table class="mdc-data-table__table" id="<%= title_id %>-table">
        <thead>
          <tr class="mdc-data-table__header-row">
            <th class="mdc-data-table__header-cell width-320" role="columnheader" scope="col">
              <i class="material-icons hide">arrow_upward</i>
              <i class="material-icons hide">arrow_downward</i>
              File
            </th>
            <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric width-75 text-center" role="columnheader" scope="col">
              <i class="material-icons hide">arrow_upward</i>
              <i class="material-icons hide">arrow_downward</i>
              % Covered
            </th>
            <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric width-60 text-center" role="columnheader" scope="col">
              <i class="material-icons hide">arrow_upward</i>
              <i class="material-icons hide">arrow_downward</i>
              Lines
            </th>
            <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric width-75 text-center" role="columnheader" scope="col">
              <i class="material-icons hide">arrow_upward</i>
              <i class="material-icons hide">arrow_downward</i>
              Relevant Lines
            </th>
            <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric width-75 text-center" role="columnheader" scope="col">
              <i class="material-icons hide">arrow_upward</i>
              <i class="material-icons hide">arrow_downward</i>
              Lines Covered
            </th>
            <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric width-75 text-center" role="columnheader" scope="col">
              <i class="material-icons hide">arrow_upward</i>
              <i class="material-icons hide">arrow_downward</i>
              Lines Missed
            </th>
            <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric width-75 text-center" role="columnheader" scope="col">
              <i class="material-icons hide">arrow_upward</i>
              <i class="material-icons hide">arrow_downward</i>
              Avg Hits/Line
            </th>
            <% if branchable_result? %>
              <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric width-95 text-center" role="columnheader" scope="col">
                <i class="material-icons hide">arrow_upward</i>
                <i class="material-icons hide">arrow_downward</i>
                Branch % Covered
              </th>
              <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric width-70 text-center" role="columnheader" scope="col">
                <i class="material-icons hide">arrow_upward</i>
                <i class="material-icons hide">arrow_downward</i>
                Branches
              </th>
              <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric width-95 text-center" role="columnheader" scope="col">
                <i class="material-icons hide">arrow_upward</i>
                <i class="material-icons hide">arrow_downward</i>
                Covered Branches
              </th>
              <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric width-95 text-center" role="columnheader" scope="col">
                <i class="material-icons hide">arrow_upward</i>
                <i class="material-icons hide">arrow_downward</i>
                Missed Branches
              </th>
            <% end %>
          </tr>
        </thead>
        <tbody class="mdc-data-table__content">
          <% files.each do |source_file| %>
            <tr class="mdc-data-table__row clickable" name="<%= id(source_file) %>"
                onclick="openModal(this.attributes.name.value)">
              <td title="<%= shortened_filename(source_file) %>" data-sort-val="<%= shortened_filename(source_file) %>">
                <div class="overflow">
                  <%= shortened_filename(source_file) %>
                </div>
              </td>
              <td class="mdc-data-table__cell mdc-data-table__cell--numeric text-center <%= coverage_class(source_file.covered_percent) %>" data-sort-val="<%= source_file.covered_percent.round(2).to_s %>">
                <%= sprintf("%.2f", source_file.covered_percent.round(2)) %>%
              </td>
              <td class="mdc-data-table__cell mdc-data-table__cell--numeric text-center" data-sort-val="<%= source_file.lines.count %>">
                <%= format_number(source_file.lines.count) %>
              </td>
              <td class="mdc-data-table__cell mdc-data-table__cell--numeric text-center" data-sort-val="<%= source_file.covered_lines.count + source_file.missed_lines.count %>">
                <%= format_number(source_file.covered_lines.count + source_file.missed_lines.count) %>
              </td>
              <td class="mdc-data-table__cell mdc-data-table__cell--numeric text-center" data-sort-val="<%= source_file.covered_lines.count %>">
                <%= format_number(source_file.covered_lines.count) %>
              </td>
              <td class="mdc-data-table__cell mdc-data-table__cell--numeric text-center" data-sort-val="<%= source_file.missed_lines.count %>">
                <%= format_number(source_file.missed_lines.count) %>
              </td>
              <td class="mdc-data-table__cell mdc-data-table__cell--numeric text-center" data-sort-val="<%= source_file.covered_strength %>">
                <%= format_number(source_file.covered_strength.round(2)) %>
              </td>
              <% if branchable_result? %>
                <td class="mdc-data-table__cell mdc-data-table__cell--numeric text-center <%= coverage_class(source_file.branches_coverage_percent) %>" data-sort-val="<%= source_file.branches_coverage_percent.round(2).to_s %>">
                  <%= sprintf("%.2f", source_file.branches_coverage_percent.round(2)) %>%
                </td>
                <td class="mdc-data-table__cell mdc-data-table__cell--numeric text-center" data-sort-val="<%= source_file.total_branches.count %>">
                  <%= format_number(source_file.total_branches.count) %>
                </td>
                <td class="mdc-data-table__cell mdc-data-table__cell--numeric text-center" data-sort-val="<%= source_file.covered_branches.count %>">
                  <%= format_number(source_file.covered_branches.count) %>
                </td>
                <td class="mdc-data-table__cell mdc-data-table__cell--numeric text-center" data-sort-val="<%= source_file.missed_branches.count %>">
                  <%= format_number(source_file.missed_branches.count) %>
                </td>
              <% end %>
            </tr>
          <% end %>
        </tbody>
    </table>
  </div>
</div>
